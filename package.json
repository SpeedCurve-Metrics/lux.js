{
  "name": "@speedcurve-metrics/lux.js",
  "version": "3.3.0",
  "scripts": {
    "build": "npm run rollup",
    "rollup": "rollup -c rollup.config.js",
    "type-check": "tsc --noEmit",
    "jest": "jest",
    "jest-watch": "jest --watch",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts' 'docs/debug-parser/**/*.ts'",
    "lint-fix": "npm run lint -- --fix",
    "test": "npm run type-check && npm run lint && npm run jest",
    "test-unit": "npm run jest /tests/unit --watchAll",
    "test-integration": "npm run jest /tests/integration --watchAll",
    "watch": "npm run rollup -- --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/expect-puppeteer": "^5.0.1",
    "@types/jest": "^28.1.1",
    "@types/jest-environment-puppeteer": "^5.0.2",
    "@types/puppeteer": "^5.4.4",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "babel-jest": "^28.0.3",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.0.3",
    "jest-puppeteer": "^6.0.0",
    "prettier": "^2.2.1",
    "puppeteer": "^15.5.0",
    "rollup": "^2.60.1",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.2.2"
  },
  "prettier": {
    "printWidth": 100
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/speedcurve-metrics"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:speedcurve-metrics/lux.js.git",
    "directory": "@speedcurve-metrics/lux.js"
  }
}
